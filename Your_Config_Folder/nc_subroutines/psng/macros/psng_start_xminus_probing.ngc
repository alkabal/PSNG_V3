o<psng_start_xminus_probing> sub

M160    ;o<clear-axis-info> call
o10 if [#<_hal[motion.probe-input]> EQ 1]
    (ABORT,!!! PROBE OR SETTER ALREADY TRIGGERED BEFORE PROBE X- !!!)
o10 endif

#<intial_X>=#<_x> (save start X position)
G91

G38.3 X-#<_psng_maxprobe_xy> F#<_psng_vel_for_search>
o500 call

  o100 if [#<_psng_latch_maxprobe> GT 0]
       G38.5 X#<_psng_latch_maxprobe> F[#<_psng_vel_for_search>*0.5]     ;DO NOT WORK FINE WITHOUT SPRING MOUNTED PROBE
       o500 call
  o100 endif

  ; Move away
  G1    X#<_psng_latch> F#<_psng_vel_for_search>

  ; Start final prove
  G38.3 X-[#<_psng_latch>*1.3] F#<_psng_vel_for_probe>                  ; *1.3 in cas of use reverse latch we need to move more than regular latch
  o500 call

#<_psng_touch_resultX> = #5061
(print,probe X=#5061)

  o200 if [#<_psng_latch_maxprobe> GT 0]
       G38.5 X#<_psng_latch_maxprobe> F[#<_psng_vel_for_search>*0.5]     ;DO NOT WORK FINE WITHOUT SPRING MOUNTED PROBE
       o500 call
  o200 endif

       G90
       ; Move away back to start point and allow a half second for the probe to clear
       G1 X#<intial_X> F#<_psng_vel_for_search>
       G4P0.5

o500 sub
     o510 if [#5070 EQ 0]
          (ABORT,PROBE X- DOES NOT FOUND THE CONTACT POINT)
     o510 endif
o500 endsub

o<psng_start_xminus_probing> endsub
M2
