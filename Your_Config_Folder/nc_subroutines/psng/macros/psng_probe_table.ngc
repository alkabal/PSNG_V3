o<psng_probe_table> sub

;  o<psng_load_var_touch_probe> call

  M160    ;o<clear-axis-info> call
  (cancel all Z offsets)
  G92.1
  G49
  G10 L20 P0 Z[#<_hal[axis.z.pos-cmd]>]
  ;G10 L2 P0 Z0 ; reset G54 offset for correct pieces measurement and replace by new value TODO TEST
  #<intial_Z>=#<_z> (save current Z position)

  ;o<backup_coord_offset> call             ; new added 2021 but removed here 2022       ; backup G54 offset for correct calc tool measurement

  o<psng_start_z_probing> call
  o500 if [#5070 EQ 0]
      (ABORT,PROBE FOR TABLE DISTANCE DOES NOT FOUND THE CONTACT POINT)
  o500 endif

  (print, probe Z=#<_psng_touch_result_Z>)

  (back to start point)
  G1 Z#<intial_Z> F#<_psng_vel_for_travel>

  G43

o<psng_probe_table> endsub
M2
